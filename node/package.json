{
  "repository": "serverless/runtime",
  "author": "Serverless, Inc.",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.{css,html,js,json,md,yaml,yml}": [
      "prettier -c"
    ]
  },
  "devDependencies": {
    "@aws-sdk/client-api-gateway": "^3.178.0",
    "@aws-sdk/client-apigatewayv2": "^3.178.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.178.0",
    "@aws-sdk/client-dynamodb": "^3.178.0",
    "@aws-sdk/client-iam": "^3.178.0",
    "@aws-sdk/client-lambda": "^3.178.0",
    "@aws-sdk/client-s3": "^3.178.0",
    "@aws-sdk/client-sns": "^3.178.0",
    "@aws-sdk/client-sqs": "^3.178.0",
    "@aws-sdk/client-sts": "^3.178.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/lint": "^17.1.0",
    "@octokit/rest": "^19.0.4",
    "@rollup/plugin-multi-entry": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@serverless/eslint-config": "^5.0.1",
    "@serverless/sdk-schema": "^0.9.0",
    "@serverless/test": "^11.0.1",
    "@serverless/utils": "^6.7.0",
    "@tsconfig/node16": "^1.0.3",
    "adm-zip": "^0.5.9",
    "chai": "^4.3.6",
    "chalk": "^4.1.2",
    "child-process-ext": "^2.1.1",
    "deasync": "^0.1.28",
    "esbuild": "^0.15.9",
    "esbuild-plugin-d.ts": "^1.1.0",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "essentials": "^1.2.0",
    "fast-glob": "^3.2.12",
    "fs2": "^0.3.9",
    "git-list-updated": "^1.2.1",
    "github-release-from-cc-changelog": "^2.3.0",
    "husky": "^4.3.8",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "log": "^6.3.1",
    "log-node": "^8.0.3",
    "memoizee": "^0.4.15",
    "mocha": "^9.2.2",
    "ncjsm": "^4.3.1",
    "node-fetch": "^2.6.7",
    "node-machine-id": "^1.1.12",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.2",
    "standard-version": "^9.5.0",
    "stream-promise": "^3.2.0",
    "timers-ext": "^0.1.7",
    "ts-proto": "^1.126.1",
    "tslib": "^2.4.0",
    "type": "^2.7.2",
    "typescript": "^4.8.3",
    "uuid": "^8.3.2",
    "yargs-parser": "^21.1.1"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:updated": "pipe-git-updated --ext=js --base=main -- eslint",
    "prettier-check": "prettier -c --ignore-path .gitignore \"**/*.{css,html,js,json,md,yaml,yml}\"",
    "prettier-check:updated": "pipe-git-updated --ext=css --ext=html --ext=js --ext=json --ext=md --ext=yaml --ext=yml --base=main -- prettier -c",
    "prettify": "prettier --write --ignore-path .gitignore \"**/*.{css,html,js,json,md,yaml,yml}\"",
    "prettify:updated": "pipe-git-updated --ext=css --ext=html --ext=js --ext=json --ext=md --ext=yaml --ext=yml --base=main -- prettier --write",
    "test:dev-mode:integration": "mocha \"test/go/dev-mode/test/integration/**/*.test.js\""
  },
  "engines": {
    "node": ">=14.0"
  },
  "license": "MIT"
}
