{
  "name": "@serverless/sdk-schema",
  "repository": "serverless/console",
  "version": "0.15.5",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha \"test/unit/**/*.test.js\"",
    "test:isolated": "mocha-isolated \"test/unit/**/*.test.js\"",
    "build": "rimraf -rf ./dist && tsc --emitDeclarationOnly && rollup --config && rimraf -rf ./dist/dts",
    "prepare-release": "standard-version && prettier --write CHANGELOG.md"
  },
  "author": "Serverless, Inc.",
  "license": "MIT",
  "dependencies": {
    "long": "^5.2.3",
    "protobufjs": "^7.2.3"
  },
  "standard-version": {
    "tagPrefix": "@serverless/sdk-schema@",
    "path": ".",
    "skip": {
      "commit": true,
      "tag": true
    },
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "perf",
        "section": "Performance Improvements"
      },
      {
        "type": "refactor",
        "section": "Maintenance Improvements"
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
