{
  "name": "@serverless/sdk",
  "repository": "serverless/console",
  "version": "0.5.21",
  "author": "Serverless, Inc.",
  "dependencies": {
    "d": "^1.0.1",
    "ext": "^1.7.0",
    "long": "^5.2.3",
    "type": "^2.7.2",
    "uni-global": "^1.0.0"
  },
  "typesVersions": {
    "*": {
      "*": [
        ".ts-types/*"
      ]
    }
  },
  "standard-version": {
    "tagPrefix": "@serverless/sdk@",
    "path": ".",
    "skip": {
      "commit": true,
      "tag": true
    },
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "perf",
        "section": "Performance Improvements"
      },
      {
        "type": "refactor",
        "section": "Maintenance Improvements"
      }
    ]
  },
  "scripts": {
    "prepare-release": "standard-version && prettier --write CHANGELOG.md",
    "test": "mocha \"test/unit/**/*.test.js\"",
    "test:isolated": "mocha-isolated \"test/unit/**/*.test.js\""
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
